window.dayDiff = function (d1, d2) {
    var days;
    days = (d2.getFullYear() - d1.getFullYear()) * 365;
    days -= (d1.getMonth() * 30);
    days += (d2.getMonth() * 30);
    days -= d1.getDate();
    days += d2.getDate();
    return days;
}

